{
  "extends": ["stylelint-config-standard", "stylelint-config-standard-scss", "stylelint-config-prettier-scss"],
  "rules": {
    "selector-class-pattern": "^[a-zA-Z][a-zA-Z0-9]*$",
    "keyframes-name-pattern": "^[a-zA-Z][a-zA-Z0-9]*$",
    "scss/at-mixin-pattern": "^[a-zA-Z][a-zA-Z0-9]*$",
    "scss/dollar-variable-pattern": "^[a-zA-Z][a-zA-Z0-9]*$"
  }
}
